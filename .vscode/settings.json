{
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.formatOnSaveMode": "file",
    "[tex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop" // Usa LaTeX Workshop como formateador
    },
    "latex-workshop.latex.autoBuild.run": "never", // Opcional: Compila automáticamente al guardar
    "latex-workshop.latex.autoClean.run": "onFailed", // Opcional: Limpia archivos auxiliares si la compilación falla
    "latex-workshop.view.pdf.viewer": "tab", // Opcional: Abre el PDF en una pestaña interna
    "window.zoomLevel": 1.3,
    "editor.fontSize": 15,
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex"
            ]
        },
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "lualatex",
            "tools": [
                "lualatex"
            ]
        },
        {
            "name": "pdflatex + biber + pdflatex",
            "tools": [
                "pdflatex",
                "biber",
                "pdflatex"
            ]
        },
        {
            "name": "xelatex + biber + xelatex",
            "tools": [
                "xelatex",
                "biber",
                "xelatex"
            ]
        },
        {
            "name": "lualatex + biber + lualatex",
            "tools": [
                "lualatex",
                "biber",
                "lualatex"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "pdflatex + biber + pdflatex",
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.formatting.latexindent.path": "latexindent", // Ruta del ejecutable (si no está en PATH)
    "latex-workshop.formatting.latexindent.args": [
        "-l", // Usa la configuración local
        "-m" // Modo silencioso
    ],
